{
    "name": "thatpodcast/decomposer",
    "description": "Decomposes Composer Packages",
    "type": "project",
    "require": {
        "php": "^7.2",
        "composer/xdebug-handler": "^1.3",
        "nikic/php-parser": "^4.2",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Beau Simensen",
            "email": "beau@dflydev.com"
        },
        {
            "name": "Dave Marshall",
            "email": "dave@atst.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Decomposer\\": "src/Decomposer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Decomposer\\Tests\\Unit\\": "tests/unit"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
